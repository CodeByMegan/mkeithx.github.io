    "@docusaurus/tsconfig": "^3.5.1",
    "@docusaurus/types": "^3.5.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "typescript": "5.4.3",
    "wrangler": "^3.62.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
